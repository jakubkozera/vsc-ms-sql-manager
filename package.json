        },
        {
          "command": "mssqlManager.deleteServerGroup",
          "when": "view == mssqlManager.explorer && viewItem == serverGroup",
          "group": "inline@3"
        },
        {
          "command": "mssqlManager.addConnectionToServerGroup",
          "when": "view == mssqlManager.explorer && viewItem == serverGroup",
          "group": "1_manage@1"
        },
        {
          "command": "mssqlManager.editServerGroup",
