{
  "name": "ms-sql-manager",
  "displayName": "MS SQL Manager",
  "description": "Manage Microsoft SQL Server databases with connection management, schema browsing, and query execution.",
  "version": "0.6.0",
  "publisher": "jakubkozera",
  "icon": "ms-sql-manager.png",
  "sponsor": {
    "url": "https://github.com/sponsors/jakubkozera"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakubkozera/vsc-ms-sql-manager.git"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "mssqlManager.sqlEditor",
        "displayName": "SQL Editor",
        "selector": [
          {
            "filenamePattern": "*.sql"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "mssqlManager.connect",
        "title": "Connect to Database",
        "icon": "$(add)"
      },
      {
        "command": "mssqlManager.executeQuery",
        "title": "Execute SQL Query",
        "icon": "$(play)"
      },
      {
        "command": "mssqlManager.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "mssqlManager.disconnect",
        "title": "Disconnect",
        "icon": "$(close)"
      },
      {
        "command": "mssqlManager.generateSelectScript",
        "title": "Generate SELECT Script",
        "icon": "$(file-code)"
      },
      {
        "command": "mssqlManager.manageConnections",
        "title": "Manage Connections",
        "icon": "$(plug)"
      },
      {
        "command": "mssqlManager.debugConnections",
        "title": "MS SQL Manager: Debug Connections",
        "icon": "$(bug)"
      },
      {
        "command": "mssqlManager.editConnection",
        "title": "Edit Connection",
        "icon": "$(edit)"
      },
      {
        "command": "mssqlManager.deleteConnection",
        "title": "Delete Connection",
        "icon": "$(trash)"
      },
      {
        "command": "mssqlManager.connectToSaved",
        "title": "Connect",
        "icon": "$(plug)"
      },
      {
        "command": "mssqlManager.createServerGroup",
        "title": "Create Server Group",
        "icon": "$(folder-library)"
      },
      {
        "command": "mssqlManager.addConnectionToServerGroup",
        "title": "Add Connection",
        "icon": "$(add)"
      },
      {
        "command": "mssqlManager.editServerGroup",
        "title": "Edit Server Group",
        "icon": "$(edit)"
      },
      {
        "command": "mssqlManager.deleteServerGroup",
        "title": "Delete Server Group",
        "icon": "$(trash)"
      },
      {
        "command": "mssqlManager.disconnectConnection",
        "title": "Disconnect",
        "icon": "$(close)"
      },
      {
        "command": "mssqlManager.selectTop1000",
        "title": "Select Top 1000",
        "icon": "$(output)"
      },
      {
        "command": "mssqlManager.scriptTableCreate",
        "title": "Script as Create",
        "icon": "$(file-code)"
      },
      {
        "command": "mssqlManager.scriptTableDrop",
        "title": "Script as Drop",
        "icon": "$(trash)"
      },
      {
        "command": "mssqlManager.refreshTable",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "mssqlManager.refreshNode",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "mssqlManager.newStoredProcedure",
        "title": "New Stored Procedure...",
        "icon": "$(add)"
      },
      {
        "command": "mssqlManager.modifyStoredProcedure",
        "title": "Modify",
        "icon": "$(edit)"
      },
      {
        "command": "mssqlManager.executeStoredProcedure",
        "title": "Execute Stored Procedure...",
        "icon": "$(play)"
      },
      {
        "command": "mssqlManager.scriptProcedureCreateToWindow",
        "title": "Script as CREATE To New Query Editor Window"
      },
      {
        "command": "mssqlManager.scriptProcedureCreateToFile",
        "title": "Script as CREATE To File"
      },
      {
        "command": "mssqlManager.scriptProcedureCreateToClipboard",
        "title": "Script as CREATE To Clipboard"
      },
      {
        "command": "mssqlManager.scriptProcedureAlterToWindow",
        "title": "Script as ALTER To New Query Editor Window"
      },
      {
        "command": "mssqlManager.scriptProcedureAlterToFile",
        "title": "Script as ALTER To File"
      },
      {
        "command": "mssqlManager.scriptProcedureAlterToClipboard",
        "title": "Script as ALTER To Clipboard"
      },
      {
        "command": "mssqlManager.scriptProcedureDropToWindow",
        "title": "Script as DROP To New Query Editor Window"
      },
      {
        "command": "mssqlManager.scriptProcedureDropToFile",
        "title": "Script as DROP To File"
      },
      {
        "command": "mssqlManager.scriptProcedureDropToClipboard",
        "title": "Script as DROP To Clipboard"
      },
      {
        "command": "mssqlManager.scriptProcedureExecuteToWindow",
        "title": "Script as EXECUTE To New Query Editor Window"
      },
      {
        "command": "mssqlManager.scriptProcedureExecuteToFile",
        "title": "Script as EXECUTE To File"
      },
      {
        "command": "mssqlManager.scriptProcedureExecuteToClipboard",
        "title": "Script as EXECUTE To Clipboard"
      },
      {
        "command": "mssqlManager.viewProcedureDependencies",
        "title": "View Dependencies",
        "icon": "$(references)"
      },
      {
        "command": "mssqlManager.renameStoredProcedure",
        "title": "Rename",
        "icon": "$(edit)"
      },
      {
        "command": "mssqlManager.deleteStoredProcedure",
        "title": "Delete",
        "icon": "$(trash)"
      },
      {
        "command": "mssqlManager.showStoredProcedureProperties",
        "title": "Properties",
        "icon": "$(info)"
      },
      {
        "command": "mssqlManager.openQueryFromHistory",
        "title": "Open Query from History",
        "icon": "$(go-to-file)"
      },
      {
        "command": "mssqlManager.pinQueryHistoryItem",
        "title": "Pin Query",
        "icon": "$(pin)"
      },
      {
        "command": "mssqlManager.unpinQueryHistoryItem",
        "title": "Unpin Query",
        "icon": "$(pin)"
      },
      {
        "command": "mssqlManager.renameQueryHistoryItem",
        "title": "Rename Query",
        "icon": "$(edit)"
      },
      {
        "command": "mssqlManager.clearQueryHistory",
        "title": "Clear Query History",
        "icon": "$(clear-all)"
      },
      {
        "command": "mssqlManager.deleteQueryHistoryItem",
        "title": "Delete History Item",
        "icon": "$(trash)"
      },
      {
        "command": "mssqlManager.refreshQueryHistory",
        "title": "Refresh Query History",
        "icon": "$(refresh)"
      },
      {
        "command": "mssqlManager.refreshSnippets",
        "title": "MS SQL Manager: Refresh SQL Snippets",
        "icon": "$(refresh)"
      },
      {
        "command": "mssqlManager.toggleQueryHistoryGrouping",
        "title": "Toggle Query History Grouping",
        "icon": "$(group-by-ref-type)"
      },
      {
        "command": "mssqlManager.newQuery",
        "title": "New Query",
        "icon": "$(new-file)"
      },
      {
        "command": "mssqlManager.newQueryOnDatabase",
        "title": "New Query",
        "icon": "$(new-file)"
      },
      {
        "command": "mssqlManager.copyConnectionString",
        "title": "Copy Connection String"
      },
      {
        "command": "mssqlManager.filterDatabases",
        "title": "Filter Databases...",
        "icon": "$(filter)"
      },
      {
        "command": "mssqlManager.filterTables",
        "title": "Filter Tables...",
        "icon": "$(filter)"
      },
      {
        "command": "mssqlManager.showDatabaseDiagram",
        "title": "Diagram",
        "icon": "$(type-hierarchy)"
      },
      {
        "command": "mssqlManager.compareSchema",
        "title": "Compare Schema",
        "icon": "$(diff)"
      },
      {
        "command": "mssqlManager.generateDatabaseScripts",
        "title": "Generate Scripts...",
        "icon": "$(file-text)"
      },
      {
        "command": "mssqlManager.exportBackup",
        "title": "Export Backup...",
        "icon": "$(export)"
      },
      {
        "command": "mssqlManager.importBackup",
        "title": "Import Backup...",
        "icon": "$(import)"
      },
      {
        "command": "mssqlManager.addAzureFirewallRule",
        "title": "Add IP to Azure Firewall",
        "icon": "$(shield)"
      },
      {
        "command": "mssqlManager.openAzurePortal",
        "title": "Open Azure Portal",
        "icon": "$(globe)"
      },
      {
        "command": "mssqlManager.clearAzureServerCache",
        "title": "Clear Azure Servers Cache"
      },
      {
        "command": "mssqlManager.showAzureServerCache",
        "title": "Show Azure Servers Cache"
      },
      {
        "command": "mssqlManager.discoverAzureServers",
        "title": "Discover Azure SQL Servers",
        "icon": "$(cloud)"
      },
      {
        "command": "mssqlManager.openSqlChat",
        "title": "Open SQL Chat",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "mssqlManager.executeChatGeneratedQuery",
        "title": "Execute Chat Generated Query"
      },
      {
        "command": "mssqlManager.insertChatGeneratedQuery",
        "title": "Insert Chat Generated Query"
      },
      {
        "command": "mssqlManager.addDatabaseInstructions",
        "title": "Add Database Instructions",
        "icon": "$(note)"
      },
      {
        "command": "mssqlManager.unlinkDatabaseInstructions",
        "title": "Unlink Database Instructions",
        "icon": "$(trash)"
      },
      {
        "command": "mssqlManager.editDatabaseInstructions",
        "title": "Edit Database Instructions",
        "icon": "$(edit)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mssqlManager",
          "title": "MS SQL Manager",
          "icon": "resources/database-dark.svg"
        }
      ]
    },
    "views": {
      "mssqlManager": [
        {
          "id": "mssqlManager.explorer",
          "name": "Database Explorer",
          "when": "true",
          "icon": "resources/database-dark.svg",
          "visibility": "visible"
        },
        {
          "id": "mssqlManager.queryHistory",
          "name": "Query History",
          "when": "true",
          "icon": "$(history)"
        }
      ]
    },
    "submenus": [
      {
        "id": "mssqlManager.scriptProcedureAs",
        "label": "Script Stored Procedure as",
        "icon": "$(file-code)"
      },
      {
        "id": "mssqlManager.scriptProcedureCreateTo",
        "label": "CREATE To",
        "icon": "$(file-code)"
      },
      {
        "id": "mssqlManager.scriptProcedureAlterTo",
        "label": "ALTER To",
        "icon": "$(edit)"
      },
      {
        "id": "mssqlManager.scriptProcedureDropTo",
        "label": "DROP To",
        "icon": "$(trash)"
      },
      {
        "id": "mssqlManager.scriptProcedureExecuteTo",
        "label": "EXECUTE To",
        "icon": "$(play)"
      }
    ],
    "chatParticipants": [
      {
        "id": "ms-sql-manager.sql",
        "name": "sql",
        "fullName": "SQL Database Assistant",
        "description": "Ask questions about your SQL databases and get help with queries",
        "isSticky": true,
        "commands": [
          {
            "name": "explain",
            "description": "Explain a SQL query or database concept"
          },
          {
            "name": "optimize",
            "description": "Suggest optimizations for a SQL query"
          },
          {
            "name": "schema",
            "description": "Show information about database tables and structure"
          }
        ],
        "disambiguation": [
          {
            "category": "database",
            "description": "The user wants help with SQL queries, database operations, or understanding database schemas.",
            "examples": [
              "Show me all customers from New York",
              "How do I join these two tables?",
              "Create a query to find the top selling products",
              "What's the structure of the users table?",
              "Help me optimize this slow query"
            ]
          }
        ]
      }
    ],
    "menus": {
      "mssqlManager.scriptProcedureAs": [
        {
          "submenu": "mssqlManager.scriptProcedureCreateTo",
          "group": "1_create"
        },
        {
          "submenu": "mssqlManager.scriptProcedureAlterTo",
          "group": "2_alter"
        },
        {
          "submenu": "mssqlManager.scriptProcedureDropTo",
          "group": "3_drop"
        },
        {
          "submenu": "mssqlManager.scriptProcedureExecuteTo",
          "group": "4_execute"
        }
      ],
      "mssqlManager.scriptProcedureCreateTo": [
        {
          "command": "mssqlManager.scriptProcedureCreateToWindow",
          "group": "1_destination@1"
        },
        {
          "command": "mssqlManager.scriptProcedureCreateToFile",
          "group": "1_destination@2"
        },
        {
          "command": "mssqlManager.scriptProcedureCreateToClipboard",
          "group": "1_destination@3"
        }
      ],
      "mssqlManager.scriptProcedureAlterTo": [
        {
          "command": "mssqlManager.scriptProcedureAlterToWindow",
          "group": "1_destination@1"
        },
        {
          "command": "mssqlManager.scriptProcedureAlterToFile",
          "group": "1_destination@2"
        },
        {
          "command": "mssqlManager.scriptProcedureAlterToClipboard",
          "group": "1_destination@3"
        }
      ],
      "mssqlManager.scriptProcedureDropTo": [
        {
          "command": "mssqlManager.scriptProcedureDropToWindow",
          "group": "1_destination@1"
        },
        {
          "command": "mssqlManager.scriptProcedureDropToFile",
          "group": "1_destination@2"
        },
        {
          "command": "mssqlManager.scriptProcedureDropToClipboard",
          "group": "1_destination@3"
        }
      ],
      "mssqlManager.scriptProcedureExecuteTo": [
        {
          "command": "mssqlManager.scriptProcedureExecuteToWindow",
          "group": "1_destination@1"
        },
        {
          "command": "mssqlManager.scriptProcedureExecuteToFile",
          "group": "1_destination@2"
        },
        {
          "command": "mssqlManager.scriptProcedureExecuteToClipboard",
          "group": "1_destination@3"
        }
      ],
      "editor/title": [
        {
          "command": "mssqlManager.executeQuery",
          "when": "resourceLangId == sql",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "mssqlManager.executeQuery",
          "when": "resourceLangId == sql",
          "group": "execution"
        }
      ],
      "view/title": [
        {
          "command": "mssqlManager.connect",
          "when": "view == mssqlManager.explorer",
          "group": "navigation@1"
        },
        {
          "command": "mssqlManager.manageConnections",
          "when": "view == mssqlManager.explorer",
          "group": "navigation@2"
        },
        {
          "command": "mssqlManager.createServerGroup",
          "when": "view == mssqlManager.explorer",
          "group": "navigation@3"
        },
        {
          "command": "mssqlManager.refreshQueryHistory",
          "when": "view == mssqlManager.queryHistory",
          "group": "navigation@1"
        },
        {
          "command": "mssqlManager.toggleQueryHistoryGrouping",
          "when": "view == mssqlManager.queryHistory",
          "group": "navigation@2"
        },
        {
          "command": "mssqlManager.clearQueryHistory",
          "when": "view == mssqlManager.queryHistory",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "mssqlManager.generateSelectScript",
          "when": "view == mssqlManager.explorer && viewItem == table",
          "group": "inline"
        },
        {
          "command": "mssqlManager.generateSelectScript",
          "when": "view == mssqlManager.explorer && viewItem == view",
          "group": "inline"
        },
        {
          "command": "mssqlManager.connectToSaved",
          "when": "view == mssqlManager.explorer && viewItem == connectionInactive",
          "group": "inline@1"
        },
        {
          "command": "mssqlManager.connectToSaved",
          "when": "view == mssqlManager.explorer && viewItem == connectionFailed",
          "group": "inline@1"
        },
        {
          "command": "mssqlManager.disconnectConnection",
          "when": "view == mssqlManager.explorer && viewItem =~ /^connectionActive/",
          "group": "inline@1"
        },
        {
          "command": "mssqlManager.editConnection",
          "when": "view == mssqlManager.explorer && viewItem =~ /^connection(Active.*|Inactive|Failed)$/",
          "group": "inline@2"
        },
        {
          "command": "mssqlManager.deleteConnection",
          "when": "view == mssqlManager.explorer && viewItem =~ /^connection(Active.*|Inactive|Failed)$/",
          "group": "inline@3"
        },
        {
          "command": "mssqlManager.newQuery",
